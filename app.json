{
  "expo": {
    "name": "Numberplate Fun",
    "slug": "numberplate-fun",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Numberplate Fun",
      "shortName": "Numberplates",
      "description": "Capture and analyze interesting numberplates",
      "lang": "en",
      "themeColor": "#ffffff",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "scope": "/numberplate-fun/",
      "startUrl": "/numberplate-fun/",
      "bundler": "webpack"
    }
  }
}
